{
  "name": "pathfinding-ui",
  "version": "1.0.0",
  "description": "A library to help with the user interface of pathfinding.js",
  "main": "src/pathfinding-ui.js",
  "scripts": {
    "build:dev": "babel src -d dist --copy-files && babel src -d docs --copy-files",
    "build:min": "BABEL_ENV=minify babel src -d dist --out-file-extension .min.js",
    "build": "npm run build:dev && npm run build:min",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarcoPlain/pathfinding-ui.js.git"
  },
  "keywords": [
    "ui",
    "ux",
    "pathfinding",
    "astar"
  ],
  "author": "Marco Schlichting (MarcoPlain)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarcoPlain/pathfinding-ui.js/issues"
  },
  "homepage": "https://github.com/MarcoPlain/pathfinding-ui.js#readme",
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "babel-preset-minify": "^0.5.2"
  }
}
